{
  "version": 2,
  "builds": [
    {
      "src": "api/app.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/models.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/authentication.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/config.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/database.py.py",
      "use": "@vercel/python"
    }
    
    
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/app.py"
    }
  ]
}